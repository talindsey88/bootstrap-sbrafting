{
  "name": "sbrafting",
  "version": "1.0.0",
  "description": "html rafting website",
  "main": "index.html",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "copyfiles": "^2.2.0",
    "imagemin-cli": "^5.1.0",
    "lite-server": "^2.5.4",
    "node-sass": "^4.14.1",
    "onchange": "^7.0.0",
    "parallelshell": "3.0.1",
    "rimraf": "^3.0.2",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "bootstrap-social": "^5.1.1",
    "dark-mode-switch": "^1.0.0",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  },
  "scripts": {
    "build": "npm run clean && npm run imagemin && npm run copyfonts && npm run usemin",
    "clean": "rimraf docs",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* docs/fonts",
    "imagemin": "imagemin img/* -o docs/img",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "start": "npm run watch:all",
    "usemin": "usemin home.html -d docs --htmlmin -o docs/home.html && usemin index.html -d docs --htmlmin -o docs/index.html",
    "test": "echo\"error: no test specified\" && exit 1",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "watch:all": "parallelshell 'npm run watch:scss' 'npm run lite'"
  }
}
